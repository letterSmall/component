<div class="BundleProductsWrapper" data-bundle-products data-discount="{{ block.settings.bundle_discount_code }}">
  	<h3>{{ block.settings.bundle_title }}</h3>
    <div class="BundleProducts">
        {% if block.settings.bundle_tag != blank %}
        	<div class="bundle_tag">{{ block.settings.bundle_tag }}</div>
        {% endif %}

        <ul class="bundles">
			{% if show_main %}
          	  {% assign discount = main.price | times: bundle_discount %}
          	  {% assign discounted_price = main.price | minus: discount %}
              {% assign total_price = total_price | plus: main.price %}
              {% assign total_sale_price = total_sale_price | plus: discounted_price %}
              <li class="bundle_product main">
                <div class="bp_img">
                  <img src="{{ main.featured_image | img_url: '200x' }}" alt="{{main.title}}" />
                </div>
                <div class="bp_info">
                  <h3 class="bp_title">{{main.title}}</h3>
                  <div class="bp_prices">
                    <span class="bp_price">{{ discounted_price | money}}</span>
                    <span class="bp_price_sale">{{ main.price | money}}</span>
                  </div>
                </div>
              </li>
          	{% endif %}
            {% for i in (1..5) %}
                {% assign bundle_product = 'bundle_product' | append: i %}
                {% if block.settings[bundle_product] != blank %}
                    {% assign product = block.settings[bundle_product] %}
                    {% assign discount = product.price | times: bundle_discount %}
                    {% assign discounted_price = product.price | minus: discount %}
                    {% assign total_price = total_price | plus: product.price %}
                    {% assign total_sale_price = total_sale_price | plus: discounted_price %}
                    <li class="bundle_product" data-bundle-variant="{{product.first_available_variant.id}}">
                        <div class="bp_img">
                            <img src="{{ product.featured_image | img_url: '200x' }}" alt="{{product.title}}" />
                        </div>
                        <div class="bp_info">
                            <h3 class="bp_title">{{product.title}}</h3>
                            <div class="bp_prices">
                                <span class="bp_price">{{ discounted_price | money }}</span>
                                <span class="bp_price_sale">{{product.price | money }}</span>
                            </div>
                        </div>
                    </li>
                {% endif %}
            {% endfor %}
        </ul>
        <div class="bp_prices total">
            <span class="bp_price">{{ total_sale_price | money }}</span>
            <span class="bp_price_sale">{{ total_price | money }}</span>
        </div>
    </div>
    <button class="btn bp-checkout" type="button">Checkout</button>
</div>


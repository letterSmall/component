<script>
  document.querySelectorAll('[data-bundle-products]').forEach(bundle => {
    let discount_code = bundle.dataset.discount;
    let btn = bundle.querySelector('.bp-checkout');
    btn.addEventListener('click', function() {
      	btn.classList.add('btn--loading');
        let formData = {
          'items': [
            {
              'id': {{main.first_available_variant.id}},
              'quantity': 1
            }
          ]
        };
    	bundle.querySelectorAll('[data-bundle-variant]').forEach(v => {
          let vid = v.dataset.bundleVariant;
          formData.items.push({'id': vid, 'quantity': 1});
        });

        fetch(window.theme.routes.cartClear, {
          credentials: 'same-origin',
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
            'X-Requested-With': 'XMLHttpRequest' // This is needed as currently there is a bug in Shopify that assumes this header

          }
        }).then(function (response) {
          fetch(window.theme.routes.cartAdd, {
            body: JSON.stringify(formData),
            credentials: 'same-origin',
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
              'X-Requested-With': 'XMLHttpRequest' // This is needed as currently there is a bug in Shopify that assumes this header

            }
          }).then(function (response) {
            if (response.ok) {
              window.location.href = '/checkout?discount=' + discount_code;
            } else {
              response.json().then(function (content) {
                var errorMessageElement = document.createElement('span');
                errorMessageElement.className = 'errors text-center';
                errorMessageElement.innerHTML = content['description'];
                btn.insertAdjacentElement('afterend', errorMessageElement);
                setTimeout(function () {
                  errorMessageElement.remove();
                }, 2500);
              });
            }
          });
        });
    });
  });
</script>

 <div class="palogo-container">
{% if section.settings.title != blank %}
  <div class="palogo-title">{{ section.settings.title }}</div>
   {% endif %}
  <div class="vac2logo-box">
   {% for block in section.blocks %}
  <div class="vac2logo-item">
    {% if block.settings.image %}
    <img class="lazyload" alt src="{{ block.settings.image | img_url:'10x' }}" data-src="{{ block.settings.image | img_url:'master' }}" /> 
   {% endif %}
   </div>
   {% endfor %}
  </div>
</div>




{% schema %}
  {
    "name": "Vac2pro Logo",
    "settings": [
   {
            "type": "text",
            "id": "title",
            "label": "title"
          }
    ],
    "blocks": [
      {
        "type": "block",
        "name": "block",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image(Recommended sizeï¼š144*70px)"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Vac2pro Logo",
        "category": "Ho-Advance"
 
      }
    ]
  }
{% endschema %}


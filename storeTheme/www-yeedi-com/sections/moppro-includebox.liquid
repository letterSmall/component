 <div class="mopproprobox-container">
   {% if section.settings.title != blank %}
  <div class="mopproprobox-title">{{ section.settings.title }}</div>
   {% endif %}
  <div class="mopproprobox-box">
   {% for block in section.blocks %}
  <div class="mopproprobox-item mopproprobox-item-{{forloop.index}}">
    {% if block.settings.image %}
    <img class="lazyload" alt src="{{ block.settings.image | img_url:'10x' }}" data-src="{{ block.settings.image | img_url:'master' }}" /> 
   {% endif %}
      {% if block.settings.title != blank %}
  <div class="mopproprobox-item-title">{{ block.settings.title }}</div>
  {% endif %}
   </div>
   {% endfor %}
  </div>
</div>




{% schema %}
  {
    "name": "Mopprobox Box",
    "settings": [
{
            "type": "text",
            "id": "title",
            "label": "title"
          }
    ],
    "blocks": [
      {
        "type": "block",
        "name": "block",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image(Recommended sizeï¼š144*70px)"
          },{
            "type": "html",
            "id": "title",
            "label": "title"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Mopprobox Box",
        "category": "Ho-Advance"
 
      }
    ]
  }
{% endschema %}


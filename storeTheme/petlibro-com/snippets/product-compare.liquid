      assign compare_content=section.settings.newcomparekei|strip
      assign compare_content_text=section.settings.newcomparekei|strip_html
     endif 
    
    assign title=compare_content|split:"</title>"|first|split:"<title>"|last|default:"Find the Right Product for You"
    assign product_list=compare_content|split:"</product>"|first|split:"<product>"|last|split:";"


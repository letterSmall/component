    }
})
$(window).resize(function(){
autoTableHeight();
})

var compare_stauts=true;
$('.mb-psc-tabbar-item[data-name="compare"]').click(function(){
  
    if($(window).width()<=768 &&compare_stauts){
      setTimeout(function(){
       autoTableHeight();
        compare_stauts=false;
      },500)
   
}
})


function autoTableHeight(){
let table=$('.pcw-table-box');

$('.pcw-table-box .pcwt-left .pcw-td').each(function(){
let that=this;
let index=$(that).data('index');
let el_height_list=[];
$(`.pcw-table-box .pcw-td-${index}`).find('.pcw-th').each(function(){
    let that2=this;
    let el_height=$(that2).outerHeight();
    el_height_list.push(el_height);

}); 
let  max = Math.max.apply(null, el_height_list);
$(`.pcw-table-box .pcw-td-${index}`).find(".pcw-th").css('min-height',`${max}px`);
  $(".product-recommended-size-container").css('opacity',1);
})
}



var pcw_pc_Swiper = new Swiper('.pc-pwc-content .pcwt-swiper-container', {
observer: true,
observeParents: true,
freeMode: true,
allowTouchMove: false,
{%assign td_size_right=item_1.size|minus:2%}

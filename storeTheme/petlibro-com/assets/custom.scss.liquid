/* how it work */
.section-how-it-work{
  .banner-wrapepr{
    position: relative;
    .image{
      display: block;
      width: 100%;
      &.mb{display: none;} 
    }
    .text-wrapper{
      position: absolute;
      width: 100%;
      padding: 0px 24px;
      bottom: 9.722222vw; 
      text-align: center;
      color: #1b1b1b;
      .title{
        font-size: 2.638888vw;
        margin-bottom: 1.6666666vw;
      }
      .content{
        font-size:1.25vw;
      }
    }
  }
  .items-wrapper-outer{
     max-width: 1440px;
    margin: 100px auto 190px;
  }
  .items-wrapper{
   
   margin:0 auto;
    padding: 0px 60px;
   
    .items-wrapper-inner{
     display: flex;
 margin-left:-30px;
    flex-wrap: wrap;
      margin-bottom:-30px;
    }
    .item{
      padding-left: 30px;
      width: calc((100%)/ 3);
    padding-bottom:30px;
    }
    .item-inner{
       border-radius: 10px;
      overflow: hidden;
    }
    .image-wrapper{
      overflow: hidden;
    }
    .item-image{
      transition: all 0.3s;
      display: block;
      &:hover{
        transform: scale(1.1);
      }
    }
    .item-title{
      font-size: 32px;
      line-height: 1.35;
      padding: 34px 30px;
      font-family: Avenir-Medium;
      letter-spacing: .03em;
      background: #ffffff;
    }
  }
  @media(max-width: 1440px){
    .items-wrapper{
      margin: 6.944444vw auto 13.19444vw;
      .item-title{
      padding: 2.36111vw 2.08333vw;
        font-size: 2.22222vw;
      }
    }
  }
  @media(max-width: 1330px){
    .items-wrapper{
      padding: 0px 24px;
    }
  }
  
  @media(max-width: 768px){
    .banner-wrapepr{
      .image{
        &.mb{display: block;}
        &.pc{display: none;}
      }
      .text-wrapper{
        top: unset;
        bottom: 22.4vw;
        .title{font-size: 8.533333vw;margin-bottom: 6.4vw;}
        .content{font-size: 4.26666vw;}
      }
    }
  }
  @media(max-width: 768px) and (min-width: 590px){
    .banner-wrapepr{
      .text-wrapper{
        bottom: 22.4vw;
        .title{font-size: 40px;margin-bottom: 40px;}
        .content{font-size: 20px;}
      }
    }
  }
  @media(max-width: 590px){
    .items-wrapper{
      padding: 0px 6.4vw;
      margin: 10.6666vw auto 24vw;
      .item{
        width: 100%;
        &:not(:last-of-type){margin-bottom: 6.4vw;}
      }
      .item-title{
        padding: 5.86666vw 6.4vw;
        font-size: 7.46666vw;
      }
    }
  }
}


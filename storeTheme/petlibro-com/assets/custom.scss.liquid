    text-align:center;
  }
  
  .videos-wrapper{
    position: relative;
    .video-item{
      position: absolute;
      top: 0px;
      opacity: 0;
      &.active{
        position: relative;
        opacity: 1;
        z-index: 1;
      }
    }
    iframe{display: block;}
  }

  .thumbs-wrapper-for-mb{
    max-width: 980px;
    margin: 20px auto 0px;
    padding: 0px 60px;
    position: relative;
  }
  .thumbs-wrapper{
    display: flex;
    justify-content: normal;
    overflow: hidden;
    
    .swiper-container-faq-video{width: 100%;}
/*     .swiper-wrapper{
      display: flex;
      justify-content: space-between;
    } */
    .thumb-item{
      width: calc(16.66666% - 10px);
      &:not(:last-of-type){
        margin-right: 12px;
      }
    }
    .item-image-wrapper{
      position: relative;
      cursor: pointer;
    }
    .item-title{
      z-index: 2;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%,-50%);
      color:#ffffff;
      font-size: 16px;
      line-height: 1.2857;
      width: 100%;
      text-align:center;
      font-family: Avenir-Heavy;
    }

    .item-image{
   display: block;
    width: 100%;
    height: 87px;
    border-radius: 5px;
    object-fit: cover;
    }
    .playing-icon{
      position: absolute;
      top: 8px;
      right: 8px;
      display: none;
    }
    .active .playing-icon{display: block;}
  }
  
  .content-wrapper{
    max-width: 980px;
    margin: 40px auto 196px;
    .item{
      padding: 30px 0px;
      position: relative;
      border-bottom: 1px solid #D6D6D6;
      &:first-of-type{
        border-top: 1px solid #D6D6D6;
      }
      .item-title{
        display: block;
        position:relative;
        font-size: 14px;
        font-family: Avenir-Heavy, Avenir;
        font-weight: bold;
        color: #1B1B1B;
        line-height: 1.35714;
      }
      .item-content{
        margin-top: 28px;
        font-size: 14px;
        font-family: Avenir-Roman, Avenir;
        font-weight: normal;
        color: #5C5C5C;
        line-height: 1.714285;
        display: none;
      }
      .icon{
        position: absolute;
        top: 0px;
        right: 0px;
        width: 16px;
        height: 16px;
        &.up{transition: all 0.3s;}
      }
      &.active{
        .icon{
          &.up{transform: rotate(180deg);}
        }
      }
    }
  }

  @media(min-width: 1921px){
    .thumbs-wrapper-for-mb{
      max-width: 51.04166vw;
    }
  }
  @media(max-width: 1440px){
    .title{
      font-size: 2.638888vw;
      margin-bottom: 1.66666vw;
    }
  }
  @media(max-width: 1100px){

{% elsif block_style == "style_5" %}

<div class="cateProductArea">
  <ul class="product-names">
    {% for childlink in linklists[link_child].links %}
    {% assign child_list_handle2 = childlink.title | handleize %}
    {% if childlink.url != blank %}          
    {% assign child_index = forloop.index %}
    <li class="product-names__item {% if forloop.first %}active {% endif %}"  id="head{{child_index}}" onmouseover="setheadTab('head',{{child_index }},{{forloop.length}})">
      <a class="product-names__link" href="{{ childlink.url  }}">
        <span>{{ childlink.title  }} </span>
      </a>
    </li>
    {% endif %}
    {% endfor %}
  </ul>

  <div class="product-images">
    {% for childlink in linklists[link_child].links %}
    {% assign child_list_handle2 = childlink.title | handleize %}
    {% if linklists[child_list_handle2].links != blank %}          
    {% assign child_index = forloop.index %}
    <div id="con_head_{{child_index}}" {% unless forloop.first %}style="display:none;" {% endunless %}>
      <ul class="product-images__list" >
        {% for grandchildlink in linklists[child_list_handle2].links %}
        {% assign child_list_handle3 = grandchildlink.title | handleize %}
        <li class="product-images__item">
          <a href="{{ grandchildlink.url }}">
            {% if grandchildlink.object.images %}
            {% for n in grandchildlink.object.images %}
            {% if forloop.first == true %}
            <div class="menu-product-image-outer">
              <img class="menu-product-image" src="{{ grandchildlink.object.images  | first | img_url: '240x' }}"/>
            </div>
            {% endif %}
            {% endfor %}
            {% else %}
            {% assign img_url3 = grandchildlink.title | handle |  prepend: 'menu-'| append:'.png' | file_img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
            <div class="menu-product-image-outer">
              <img class="menu-product-image" src=" {{ grandchildlink.title | handle |  prepend: 'menu-product-'| append:'.png'| file_img_url: '240x' }}" alt="" onerror="this.src='{{ 'nav-bg'| append: '.png' | asset_url }};this.onerror=null'"/> 
            </div>
            {% endif %}    
            <span>{{ grandchildlink.title }}</span>
          </a>
        </li>                           
        {% endfor %}                                                       
      </ul>  
    </div>
    {% endif %}
    {% endfor %}
  </div>
</div>


{% if block_style == "style_1" %}
    <div class="cateArea colLeft">
        <ul class="site-nav-dropdowns columns-3">
            {% for l in linklists[link_child].links %}
            <li class="menu-lv-2 {% if linklists[link_child] != empty %} dropdown{% endif %}">
                <p class="nav-action{%if linklists[link_child] != empty %} menu__moblie{% else %} menu__moblie_end{% endif %}">
                    <a href="{{ l.url }}" class="current link" aria-label="link">
                        {{ l.title }}
                    </a>
                    {% if linklists[l.handle] != empty %}
                        <span class="icon-dropdown" data-toggle-menu-mb>&#10095;</span>
                    {% endif %}
                </p>
                {% if linklists[l.handle] != empty %}
                <div class="sub-menu-mobile ">
                    <div class="menu-mb-title">
                        <span class="icon-dropdown">&#10094;</span>
                        {{ l.title }}
                    </div>
                    <ul class="site-nav-dropdown">
                        {% for child_l in linklists[l.handle].links %}
                        <!-- Menu level 3 -->
                        <li class="menu-lv-3">
                            <a href="{{ child_l.url }}" class="nav-action menu__moblie_end {% if child_l.active %}current{% endif %}" aria-label="link">
                                <span>{{ child_l.title }}</span>
                            </a>
                        </li>
                        {% endfor %}
                    </ul>
                </div>
                {% endif %}
            </li>
            {% endfor %}
        </ul>
    </div>
    <div class="imageArea colRight">
        <div class="imgList">
            <div class="imgItem imgLeft">
                {% if title_featured_product != blank %}
                <h3 class="title">
                    {{ title_featured_product }}
                </h3>
                {% endif %}
                <div class="featuredProductCarousel">
                    {% if featured_product != blank %}
                        <div class="items">
                            {% render 'product-card-grid-2', product : product %}
                        </div>
                    {% else %}
                        <div class="items">
                            <div class="product-card">
                                <div class="product-image">
                                    <a class="product-link" href="#">
                                        {{ 'product-2' |  placeholder_svg_tag: 'placeholder-svg' }}
                                    </a>
                                </div>
                                <div class="product-content">
                                    <h4 class="product-title">{{ 'homepage.onboarding.product_title' | t }}</h4>
                                    {% assign price = 1999 %}
                                    <div class="product-price" data-price>
                                        <span class="price-item price-item--regular" data-regular-price>
                                            {{ price | money }}
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endif %}
                    {% if featured_product_2 != blank %}
                        <div class="items">
                            {% render 'product-card-grid-2', product : product_2 %}
                        </div>
                    {% endif %}
                    {% if featured_product_3 != blank %}
                        <div class="items">
                            {% render 'product-card-grid-2', product : product_3 %}
                        </div>
                    {% endif %}
                </div>
            </div>
            {% if banner_first_style != blank %}
                <div class="imgItem imgRight text-center">
                    {% if link_banner_first_style %}
                        <a href="{{ link_banner_first_style }}" aria-label="link">
                            <img class="lazyload" src="{{ 'loading.svg' | asset_url }}" data-src="{{ banner_first_style | img_url : '270x' }}" alt="" data-image>
                        </a>
                    {% else %}
                        <img class="lazyload" src="{{ 'loading.svg' | asset_url }}" data-src="{{ banner_first_style | img_url : '270x' }}" alt="" data-image>
                    {% endif %}
                </div>
            {% else %}
                <div class="imgItem imgRight text-center">
                    <span class="recommended_size">270x270</span>
                    {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
                </div>
            {% endif %}
        </div>
    </div>

/* Feedback Modify */
/* Home Page PC */
.cateProductArea .product-images__list .product-images__item .menu-product-image-outer {
  display: block;
  margin: auto auto 10px;
  max-width: 200px;
  padding: 20px;
  background: transparent;
  border-radius: 10px;
  position: relative;
}
.cateProductArea .product-images__list .product-images__item .menu-product-image-outer::after {
  display: block;
  content: "";
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  margin: auto;
  background: rgba(0,0,0,.05);
  border-radius: 10px;
}
.cateProductArea .product-images__list .product-images__item img {
  display: block;
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  background: transparent;
  border-radius: 10px;
}
.custom-block-list .customBlock_item::after {
  position: absolute;
  left: -50%;
}
@media only screen and (max-width: 1400px) {
  .custom-block-list .customBlock_item::after {
    position: absolute;
    left: -45%;
  }
}
@media only screen and (max-width: 1300px) {
  .custom-block-list .customBlock_item::after {
    position: absolute;
    left: -38%;
  }
}
/* Home Page Mobile */
.mobileMenu-toggle .mobileMenu-toggleIcon, .mobileMenu-toggle .mobileMenu-toggleIcon::before, .mobileMenu-toggle .mobileMenu-toggleIcon::after {
  border-radius: 50px;
}
.halo-header-mobile .navUser-action[data-cart-preview] .countPill {


  <div class="palogo-box">
   {% for block in section.blocks %}
  <div class="palogo-item">
    {% if block.settings.image %}
    <img alt src="{{ block.settings.image | img_url:'master' }}" width="{{ block.settings.image.width }}" height="{{ block.settings.image.height }}" /> 
   {% endif %}
   </div>
   {% endfor %}
  </div>





